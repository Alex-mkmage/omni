<?xml version="1.0" encoding="UTF-8"?>
<layout>
    <!-- Default -->
    <default>
        <reference name="head">
            <action method="addJs"><script>lib/jquery/jquery-1.10.2.min.js</script></action>
            <!--<action method="addItem"><type>skin_js</type><name>js/jquery-migrate-3.0.0.min.js</name></action>-->
            <action method="addJs"><script>lib/jquery/noconflict.js</script></action>
            <action method="addItem"><type>skin_js</type><name>js/hammer.js</name></action>
            <action method="addItem"><type>skin_js</type><name>js/jquery.main.js</name></action>
            <action method="addItem"><type>skin_js</type><name>js/jquery.navmenu.js</name></action>
            <action method="addItem"><type>skin_js</type><name>js/jquery.fancybox.js</name></action>
            <action method="addItem"><type>skin_js</type><name>js/enquire.js</name></action>
            <!-- add the local stylesheet -->
            <action method="addItem"><type>skin_css</type><name>css/header-styles.css</name></action>
            <!-- <action method="addLinkRel"><rel>stylesheet</rel><href>//maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css</href></action> -->

            <!-- add the local js -->
            <action method="addItem"><type>skin_js</type><name>js/ie.js</name><params/><if> lt IE 9</if></action>
            <action method="addLinkRel"><rel>stylesheet</rel><href>//fonts.googleapis.com/css?family=Open+Sans:400,300,700</href></action>
        </reference>
        <reference name="header">
            <block type="cms/block" name="header-menu" as="header-menu">
                <action method="setBlockId"><block_id>header-menu</block_id></action>
            </block>
            <block type="cms/block" name="header-chat" as="header-chat">
                <action method="setBlockId"><block_id>header-chat</block_id></action>
            </block>

            <block type="core/template" name="distributor_location" template="distributor/location.phtml"/>
            <block type="checkout/cart_sidebar" name="cart_sidebar" template="checkout/cart/sidebar.phtml" before="-">
                <action method="addItemRender">
                    <type>simple</type>
                    <block>checkout/cart_item_renderer</block>
                    <template>checkout/cart/sidebar/default.phtml</template>
                </action>

                <action method="addItemRender"><type>grouped</type><block>checkout/cart_item_renderer_grouped</block><template>checkout/cart/sidebar/default.phtml</template></action>
                <action method="addItemRender"><type>configurable</type><block>checkout/cart_item_renderer_configurable</block><template>checkout/cart/sidebar/default.phtml</template></action>
            </block>
            <block type="qquoteadv/sidebar" name="quote_link" template="qquoteadv/top-link.phtml" after="cart_sidebar"/>
        </reference>
        <reference name="after_body_start">
            <block type="core/template" name="tagmanager" template="page/html/tagManager.phtml" />
            <!-- <block type="core/template" name="liveEngage" template="page/html/liveEngage.phtml" /> -->
        </reference>
        <reference name="footer">
            <block type="cms/block" name="footer-menu" as="footer-menu">
                <action method="setBlockId"><block_id>footer-menu</block_id></action>
            </block>
            <block type="cms/block" name="footer-dynamics" as="footer-dynamics">
                <action method="setBlockId"><block_id>footer-dynamics</block_id></action>
            </block>
        </reference>
        <!-- Remove -->
        <reference name="right">
            <action method="unsetChild"><name>right.permanent.callout</name></action>
            <action method="unsetChild"><name>catalog.compare.sidebar</name></action>
            <action method="unsetChild"><name>cart_sidebar</name></action>
            <remove name="paypal.partner.right.logo"/>
            <action method="unsetChild"><name>right.reports.product.viewed</name></action>
            <action method="unsetChild"><name>right.reports.product.compared</name></action>
            <remove name="sale.reorder.sidebar"/>
            <remove name="wishlist_sidebar"/>
            <action method="unsetChild"><name>right.poll</name></action>
            <action method="unsetChild"><name>quote_sidebar</name></action>
        </reference>
        <reference name="left">
            <action method="unsetChild"><name>left.permanent.callout</name></action>
            <action method="unsetChild"><name>tags_popular</name></action>
            <action method="unsetChild"><name>left.newsletter</name></action>
        </reference>
        <reference name="root">
            <block type="core/template" name="page_title" as="page_title" template="page/html/page-title.phtml"/>
        </reference>
    </default>

